{
  "name": "azure-pipelines-html-report",
  "version": "0.0.1",
  "private": true,
  "author": "Niels Ebbelink",
  "dependencies": {
    "azure-devops-extension-api": "^1.157.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^2.165.1",
    "dashify": "^2.0.0",
    "jszip": "^3.10.1",
    "material-table": "^1.60.0",
    "mustache": "^3.0.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^9.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-inspector": "^6.0.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.30",
    "@types/react": "^17.0.74",
    "@types/react-dom": "^18.2.18",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "jsdom": "^16.2.1",
    "rimraf": "^5.0.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^1.1.3",
    "tfx-cli": "^0.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist ./bin",
    "precompile": "npm run clean",
    "compile": "npx webpack && cd ./PublishHtmlReport/ && tsc",
    "compile:dev": "npx webpack --mode=development",
    "install-dependencies": "npm install && cd PublishHtmlReport && npm install && cd ..",
    "package": "npm run compile && npx tfx-cli extension create --rev-version --json5 --output-path bin",
    "publish": "npm run compile && npx tfx-cli extension publish --rev-version --json5 --manifest-globs vss-extension.json src/extensions/**/*.json --output-path bin --token %ADO_MARKETPLACE_PATH%"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
